{
    "name": "Flyff Buttons",
    "description": "Auto buttons for Flyff Universe",
    "version": "0.0.5",
    "manifest_version": 3,
    "permissions": [],
    "content_scripts": [
        {
            "js": ["inject.js"],
            "matches": ["*://universe.flyff.com/play*"]
        }
    ],
    "icons": {
        "128": "logo.png",
        "16": "logo.png",
        "48": "logo.png"
    },
    "web_accessible_resources": [
        {
            "resources": ["flyff.js"],
            "matches": [
                "https://universe.flyff.com/*",
                "http://universe.flyff.com/*"
            ],
            "use_dynamic_url": true
        }
    ]
}
