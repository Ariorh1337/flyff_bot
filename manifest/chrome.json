{
    "name": "Flyff Buttons",
    "description": "Auto buttons for Flyff Universe",
    "version": "0.0.3",
    "manifest_version": 3,
    "permissions": [],
    "content_scripts": [
        {
            "js": ["flyff.js"],
            "matches": ["*://universe.flyff.com/play*"]
        }
    ],
    "icons": {
        "128": "logo.png",
        "16": "logo.png",
        "48": "logo.png"
    }
}
